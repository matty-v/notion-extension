{
  "manifest_version": 3,
  "name": "Notion Helper",
  "description": "Helper that integrates with your Notion notes",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "js": ["./static/js/content.js"],
      "matches": ["https://*/*"],
      "all_frames": false,
      "run_at": "document_end"
    }
  ],
  "permissions": ["tabGroups"]
}
